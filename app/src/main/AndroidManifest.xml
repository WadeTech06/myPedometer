<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cis470.lakiel.mypedometer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CustomAlgoResultsActivity"
            android:label="Navigates to custom" />
        <activity
            android:name=".StepsCounterActivity"
            android:label="Navigates to step counter " />
        <activity
            android:name=".StepsHistoryActivity"
            android:label="Navigates to history activity" />
    </application>

</manifest>